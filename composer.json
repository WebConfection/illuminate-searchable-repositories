{
    "name": "webconfection/illuminate-searchable-repositories",
    "description": "Illuminate database package specifically designed to facilitate the easy creation of searchable API endpoints by accepting multi-dimensional arrays to build a select statement",
    "keywords": ["illuminate repositories laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Charles Jackson",
            "email": "charles.jackson@jacksoncharles.co.uk"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "4.0.*",
        "illuminate/events": "^5.2",
        "illuminate/database": "^5.2",
        "mockery/mockery": "dev-master@dev",
        "robmorgan/phinx": "^0.6.4",
        "fzaninotto/faker": "^1.6",
        "illuminate/pagination": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "WebConfection\\Repositories\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebConfection\\Repositories\\Tests\\": "tests/"
        }
    }
}
